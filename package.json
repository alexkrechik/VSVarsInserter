{
  "name": "VSVarsInserter",
  "displayName": "VSVarsInserter",
  "description": "Insert all the vars, declared via 'var' or 'let', into return block",
  "version": "0.2.0",
  "publisher": "AlexanderKrechik",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.insertVars"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.insertVars",
        "title": "VarsInserting into return block"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "jscs": "^3.0.7"
  }
}
